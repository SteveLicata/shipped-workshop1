<!DOCTYPE html>
<html>
  <head>
    <title>ShippedWorkshop</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <nav>
      <% if (company_signed_in?) %>
        <%= link_to 'Company Sign Out', { :action=>"destroy", :controller =>"devise/sessions" }, method: :delete %>

      <% else %>
        <%= link_to 'Company Sign In', new_company_session_path %>
        <%= link_to 'Company Sign Up', new_company_registration_path%>
      <% end %>
    </nav>




    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <%= yield %>
  </body>
</html>
